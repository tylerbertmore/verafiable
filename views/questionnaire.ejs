                <p>Outdoorsy?</p>
            <select name="outdoorsy">
                <option value="" disabled selected>Choose One</option>
                <option value="Yes">Yes</option>
                <option value="No">No</option>

            </select>
        </label>
        
            <label for="outdoorActivities" >
                <p>Fav Outdoor Activities?</p>
                <input type="text" name="outdoorActivities" placeholder="Enter Activities" required>
            </label>
        
        </div>

         <!-- Prev MArriage -->
         <div>
