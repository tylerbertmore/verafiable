<%- include('./partials/header') %> 
<div id="questionnaire">
    <h1>Questionnaire</h1>
    
    
    <form id="questionnaire-form" action="/questionnaire" method="POST">
        <!-- STRING INPUT -->
        <div>
            <label for="fullName" >
                <p>Full Name</p>
                <input type="text" name="fullName" placeholder="Enter Full Name" required>
            </label>
        </div>
        <!-- DATE -->
        <div>
            <label for="birthday" >
                <p>Birthday</p>
                <input type="date" min="1903-01-02" max="2004-01-01" name="birthday" placeholder="Birthday" required>
            </label>
        </div>
        <!-- Number INPUT -->
        <!-- <div>
            <label for="age" > 
                <p>Age</p>
                <input type="number" name="age" placeholder="Enter Age" required>
            </label>
        </div> -->
        <!-- Originally From? -->
        <div>
            <label for="originallyFrom" >
                <p>Originally From?</p>
                <input type="text" name="originallyFrom" placeholder="Enter Place" required>
            </label>
        <div>
            <p>Select Gender:</p>
            <label for="Male">Male
            <input type="radio" name="gender" id="Male" value="Male"></label>
            <label for="Female">Female
            <input type="radio" name="gender" id="Female" value="Female"></label>
            <label for="Other">Other
            <input type="radio" name="gender" id="Other" value="Other"></label>
        </div>
        
        </div>
        <!-- Seeking -->
        <div>
            <label for="seeking">
                <p>Seeking:</p>
            <select name="seeking">
                <option value="" disabled selected>Choose One</option>
              <option value="Men">Men</option>
              <option value="Women">Women</option>
              <option value="Both">Both</option>
            </select>
        </label>
        </div>
        <!-- ageRange -->
        <div>
            <label for="ageRange">
                <p>Ideal Partner Age:</p>
            <select name="ageRange">
                <option value="" disabled selected>Choose One</option>
              <option value="20-25">20-25</option>
              <option value="25-30">25-30</option>
              <option value="30-35">30-35</option>
              <option value="35-40">35-40</option>
              <option value="40-45">40-45</option>
              <option value="45-50">45-50</option>
              <option value="50-55">50-55</option>
              <option value="60-65">60-65</option>
            </select>
        </label>
        </div>
        <!-- Ethnicity -->
        <div>
            <label for="ethnicity">
                <p>Ethnicity</p>
            <select name="ethnicity">
                <option value="" disabled selected>Choose One</option>
              <option value="White">White</option>
              <option value="Black">Black</option>
              <option value="Asian">Asian</option>
              <option value="Hispanic">Hispanic</option>
              <option value="Middle-Eastern">Middle-Eastern</option>
            </select>
        </label>
        </div>

        <!-- Religion -->
        <div>
            <label for="religion">
                <p>Religion</p>
            <select name="religion">
                <option value="" disabled selected>Choose One</option>
                <option value="Christian">Christian</option>
                <option value="Catholic">Catholic</option>
                <option value="Jewish">Jewish</option>
                <option value="Buddhist">Buddhist</option>
                <option value="Mormon">Mormon</option>
                <option value="Islamic">Islamic</option>
                <option value="Spiritual">Spiritual</option>
                <option value="Agnostic">Agnostic</option>
            </select>
        </label>
        </div>
        <!-- Politics -->
        <div>
            <label for="politics">
                <p>Political Views?</p>
            <select name="politics">
                <option value="" disabled selected>Choose One</option>
                <option value="Not Political">Not Political</option>
                <option value="Liberal">Liberal</option>
                <option value="Conservative">Conservative</option>
                <option value="Moderate">Moderate</option>
                <option value="Other">Other</option>

            </select>
        </label>
        </div>

                <!-- Children -->
                <div>
                    <label for="children">
                        <p>Do You Have Children?</p>
                    <select name="children">
                        <option value="" disabled selected>Choose One</option>
                        <option value="Yes">Yes</option>
                        <option value="No">No</option>
                        
                    </select>
                </label>

                    <label for="wantChildren">
                        <p>Do You Want Children?</p>
                    <select name="wantChildren">
                        <option value="" disabled selected>Choose One</option>
                        <option value="Yes">Yes, I want Children</option>
                        <option value="No">No, I don't want Children</option>
                    </select>
                </label>
                
                    <label for="stepParent">
                        <p>Willing to be a Step Parent?</p>
                    <select name="stepParent">
                        <option value="" disabled selected>Choose One</option>
                        <option value="Yes">Yes</option>
                        <option value="No">No</option>
                    </select>
                </label>
                </div>
        <!-- Alcohol -->
        <div>
            <label for="alcoholUse">
                <p>Alcohol Consumption</p>
            <select name="alcoholUse">
                <option value="" disabled selected>Choose One</option>
              <option value="Never">I don't Drink Alcohol</option>
              <option value="Infrequently">Once in a While</option>
              <option value="Occasionally">Occasionally</option>
              <option value="Socially">Socially</option>
              <option value="Often">Often</option>
              <option value="Excessively">Excessively</option>
            </select>
        </label>
        </div>
        <!-- Cigarette -->
        <div>
            <label for="cigaretteUse">
                <p>Cigarette Use</p>
            <select name="cigaretteUse">
                <option value="" disabled selected>Choose One</option>
              <option value="Never">I don't Smoke</option>
              <option value="Infrequently">Once in a While</option>
              <option value="Occasionally">Occasionally</option>
              <option value="Socially">Socially</option>
              <option value="Often">Often</option>
              <option value="Excessively">Excessively</option>
            </select>
        </label>
        </div>
        <!-- Marijuana -->
        <div>
            <label for="marijuanaUse">
                <p>Marijuana Usage</p>
            <select name="marijuanaUse">
                <option value="" disabled selected>Choose One</option>
              <option value="Never">I don't Smoke Marijuana</option>
              <option value="Infrequently">Once in a While</option>
              <option value="Occasionally">Occasionally</option>
              <option value="Socially">Socially</option>
              <option value="Often">Often</option>
              <option value="Excessively">Excessively</option>
            </select>
        </label>
        </div>
        <!-- Exercise -->
        <div>
            <label for="exercise">
                <p>Do you Exercise?</p>
            <select name="exercise">
                <option value="" disabled selected>Choose One</option>
              <option value="Never">Never / Almost Never</option>
              <option value="Monthly">A Few Times a Month</option>
              <option value="Weekly">1-2 Times a Week</option>
              <option value="Often">3-4 Times a Week</option>
              <option value="Very Often">5-7 Times a Week</option>
            </select>
        </label>
        </div>
        <!-- Tattoos -->
        <div>
            <label for="tattoos">
                <p>Tattoos?</p>
            <select name="tattoos">
                <option value="" disabled selected>Choose One</option>
              <option value="Yes">Yes, I have Tattoos</option>
              <option value="No">No, I have no Tattoos</option>

            </select>
        </label>
        <fieldset>      
            <legend>Where are your Tattoos?</legend>      
            <input type="checkbox" name="tattooPlaces" value="Face">Face<br>      
            <input type="checkbox" name="tattooPlaces" value="Neck">Neck<br>      
            <input type="checkbox" name="tattooPlaces" value="Shoulders">Shoulders<br>      
            <input type="checkbox" name="tattooPlaces" value="Hands">Hands<br>      
            <input type="checkbox" name="tattooPlaces" value="Arms">Arms<br>      
            <input type="checkbox" name="tattooPlaces" value="Legs">Legs<br>      
            <input type="checkbox" name="tattooPlaces" value="Feet">Feet<br>      
            <input type="checkbox" name="tattooPlaces" value="Back">Back<br>      
            <input type="checkbox" name="tattooPlaces" value="Stomach">Stomach<br>      
            <br>      
                 
