        </div>
        <!-- Partner Tattoos -->
        <div>
            <label for="partnerTattoos">
                <p>Partner Tattoos?</p>
            <select name="partnerTattoos">
                <option value="" disabled selected>Choose One</option>
              <option value="Yes">Yes, I'm okay with them</option>
              <option value="No">No, I'm not okay with them</option>

